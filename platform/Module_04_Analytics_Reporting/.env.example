# Application Configuration
NODE_ENV=development
PORT=3004
API_PREFIX=api/v1

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5435/analytics_db
DB_HOST=localhost
DB_PORT=5435
DB_USERNAME=postgres
DB_PASSWORD=password
DB_DATABASE=analytics_db
DB_SYNCHRONIZE=true
DB_LOGGING=true

# ClickHouse Configuration
CLICKHOUSE_URL=clickhouse://localhost:9000/analytics
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=9000
CLICKHOUSE_DATABASE=analytics
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=
CLICKHOUSE_HTTP_PORT=8123

# Redis Configuration
REDIS_URL=redis://localhost:6380
REDIS_HOST=localhost
REDIS_PORT=6380
REDIS_PASSWORD=

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=24h
VALID_API_KEYS=sk-analytics-key,sk-analytics-prod-key

# Analytics Configuration
ANALYTICS_RETENTION_DAYS=365
REAL_TIME_ANALYTICS_ENABLED=true
AI_INSIGHTS_ENABLED=true
ANOMALY_DETECTION_ENABLED=true
DASHBOARD_CACHE_TTL=300
REPORT_GENERATION_TIMEOUT=600000
MAX_CONCURRENT_REPORTS=10
ANALYTICS_BATCH_SIZE=1000

# Power BI Configuration
POWERBI_CLIENT_ID=your-powerbi-client-id
POWERBI_CLIENT_SECRET=your-powerbi-client-secret
POWERBI_TENANT_ID=your-powerbi-tenant-id
POWERBI_WORKSPACE_ID=your-powerbi-workspace-id
POWERBI_DATASET_ID=your-powerbi-dataset-id
POWERBI_REPORT_ID=your-powerbi-report-id

# WhatsApp BI Configuration
WHATSAPP_API_KEY=your-whatsapp-api-key
WHATSAPP_WEBHOOK_URL=https://your-domain.com/webhooks/whatsapp
WHATSAPP_PHONE_NUMBER=+1234567890
WHATSAPP_BUSINESS_ACCOUNT_ID=your-business-account-id

# AI Configuration
AI_SERVICE_ENABLED=true
AI_MODEL_PATH=./models
AI_CONFIDENCE_THRESHOLD=0.8
AI_MAX_CONCURRENT_REQUESTS=10
NLP_SERVICE_ENABLED=true
NLP_MODEL_PATH=./nlp-models

# Queue Configuration (BullMQ)
BULL_REDIS_HOST=localhost
BULL_REDIS_PORT=6380
BULL_REDIS_PASSWORD=
QUEUE_CONCURRENCY=10
QUEUE_MAX_ATTEMPTS=3
QUEUE_BACKOFF_TYPE=exponential
QUEUE_BACKOFF_DELAY=2000

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_SKIP_SUCCESSFUL_REQUESTS=false

