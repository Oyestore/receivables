# Business Configuration
DEFAULT_TIMEZONE=UTC
DEFAULT_DATE_FORMAT=YYYY-MM-DD
DEFAULT_CURRENCY=USD
MAX_QUERY_TIMEOUT=30000
MAX_EXPORT_RECORDS=100000

# Analytics Configuration
ANALYTICS_CACHE_ENABLED=true
ANALYTICS_CACHE_TTL=3600
ANALYTICS_REAL_TIME_ENABLED=true
ANALYTICS_PREDICTION_ENABLED=true
ANALYTICS_ANOMALY_DETECTION_ENABLED=true

# Dashboard Configuration
DASHBOARD_MAX_WIDGETS=50
DASHBOARD_MAX_USERS=100
DASHBOARD_VERSIONING_ENABLED=true
DASHBOARD_COLLABORATION_ENABLED=true
DASHBOARD_EXPORT_ENABLED=true

# Report Configuration
REPORT_MAX_ROWS=1000000
REPORT_MAX_COLUMNS=100
REPORT_EXPORT_FORMATS=pdf,xlsx,csv,json
REPORT_SCHEDULE_ENABLED=true
REPORT_EMAIL_ENABLED=true

# Performance Configuration
