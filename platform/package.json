    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": "",
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/dist/",
            "/coverage/",
            "/code/",
            "<rootDir>/Module_02_Intelligent_Distribution/",
            "<rootDir>/Module_04_Analytics_Reporting/src/tests/e2e/",
            "<rootDir>/Module_04_Analytics_Reporting/src/tests/integration/",
            "<rootDir>/Module_11_Common/tests/",
            "<rootDir>/Module_06_Credit_Scoring/",
            "<rootDir>/Module_08_Dispute_Resolution_&_Legal_Network/",
            "<rootDir>/test/",
            "<rootDir>/Module_05_Milestone_Workflows/src/services/__tests__/",
            "<rootDir>/Module_05_Milestone_Workflows/src/controllers/__tests__/",
            "<rootDir>/Module_05_Milestone_Workflows/src/tests/",
            "<rootDir>/Module_12_Administration/src/services/__tests__/",
            "<rootDir>/Module_13_Cross_Border_Trade/src/services/__tests__/",
            "<rootDir>/Module_07_Financing/src/tests/",
            "<rootDir>/Module_04_Analytics_Reporting/src/services/__tests__/",
            "<rootDir>/Module_04_Analytics_Reporting/src/controllers/__tests__/",
            "<rootDir>/Module_04_Analytics_Reporting/src/tests/",
            "<rootDir>/Module_07_Financing/src/services/__tests__/",
            "<rootDir>/Module_17_Reconciliation_GL/src/services/erp/__tests__/"
        ],
        "testMatch": [
            "<rootDir>/**/__tests__/**/*.spec.ts",
            "<rootDir>/**/src/**/*.spec.ts"
        ],
        "transform": {
            "^.+\\.(t|j)s$": [
                "ts-jest",
                {
                    "isolatedModules": true
                }
            ]
        },
        "collectCoverageFrom": [
            "src/**/*.(t|j)s",
            "Module_01_*/src/**/*.(t|j)s",
            "Module_02_*/src/**/*.(t|j)s",
            "Module_03_*/src/**/*.(t|j)s",
            "Module_04_*/src/**/*.(t|j)s",
            "Module_05_*/src/**/*.(t|j)s",
            "Module_06_*/src/**/*.(t|j)s",
            "Module_07_*/src/**/*.(t|j)s",
            "Module_11_*/src/**/*.(t|j)s",
            "Module_12_*/src/**/*.(t|j)s",
            "Module_13_*/src/**/*.(t|j)s",
            "Module_15_*/src/**/*.(t|j)s",
            "Module_17_*/src/**/*.(t|j)s",
            "test/**/*.(t|j)s",
            "!**/frontend_assets/**",
            "!**/code/**",
            "!**/*.stub.ts"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/dist/",
            "/coverage/",
            "/frontend_assets/",
            "/code/",
            "Module_07_Financing_Factoring/",
            "<rootDir>/Module_11_Common/tests/",
            "<rootDir>/Module_06_Credit_Scoring/",
            "<rootDir>/Module_08_Dispute_Resolution_&_Legal_Network/",
            "<rootDir>/test/"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node"
    }
